{
	"extends": "airbnb-base",
	"parser": "@babel/eslint-parser",
	"env": {
	  "browser": true
	},
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["gulpfile.babel.js", "gulp/**/*", "main.js"]}],
	"linebreak-style": ["error", (process.platform === "win32" ? "windows" : "unix")],
  }
}
